page {
  background-color: #f0f0f0;
}

.edit {
  margin-top: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  text-align: right;
  background-color: #fff;
}

.cart-empty {
  text-align: center;
  font-size: 30rpx;
}

.cart-empty image {
  margin-top: 100rpx;
  margin-bottom: 20rpx;
}

.cart-empty .cart-empty-font2 {
  margin: 0 auto;
  margin-top: 20rpx;
  text-align: center;
  line-height: 80rpx;
  background-color: #aaa;
  color: #fff;
  border-radius: 10rpx;
}

.buy-goods {
  position: relative;
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #fff;
  font-size: 27rpx;
  border-bottom: 2rpx solid #ddd;
}

.buy-goods .checkbox {
  position: absolute;
  top: 80rpx;
}

.buy-goods .checkbox checkbox {
  border-radius: 50%;
}

.buy-goods .goods-img {
  position: absolute;
  top: 20rpx;
  left: 80rpx;
}

.buy-goods .goods-name {
  position: absolute;
  top: 20rpx;
  left: 300rpx;
}

.sell {
  font-size: 25rpx;
  color: red;
  margin-left: 20rpx;
}

.buy-goods .goods-pricee {
  position: absolute;
  top: 105rpx;
  left: 300rpx;
  color: red;
}

.number {
  position: absolute;
  top: 80rpx;
  left: 470rpx;
  width: 100%;
  height: 50rpx;
  padding: 20rpx;
  font-size: 40rpx;
  line-height: 50rpx;
  color: #777;
}

.number2 {
  position: absolute;
  top: 45rpx;
  left: 480rpx;
  width: 100%;
  height: 50rpx;
  padding: 20rpx;
  font-size: 40rpx;
  line-height: 50rpx;
  color: #777;
  /* border: 1px solid red;  */
}

.jian, .num, .jia {
  border: 2rpx solid #ccc;
  text-align: center;
  line-height: 50rpx;
  font-size: 50rpx;
  float: left;
}

.jian {
  margin-left: 30rpx;
}

.num {
  /* width: 80rpx; */
  border-left: 0;
  border-right: 0;
  font-size: 30rpx;
}

.tishi {
  position: absolute;
  top: 190rpx;
  left: 300rpx;
  font-size: 25rpx;
  color: #ddd;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.youlike {
  font-size: 28rpx;
  text-align: center;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  /* border:1px solid red;  */
}

.youlike .youlike-line1, .youlike-line2 {
  height: 0;
  margin-top: 50rpx;
  border: 1rpx solid #aaa;
}

.goods-info {
  margin: 0 15rpx;
  margin-bottom: 20rpx;
  /* margin-left: 20rpx; */
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  text-align: center;
  background-color: #fff;
}

.goods-info .goods-name {
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.goods-info .goods-price {
  margin-top: 20rpx;
  text-align: left;
  color: red;
}

.commit-cart {
  position: fixed;
  z-index: 9999;
  font-size: 30rpx;
  display: flex;
  flex-wrap: nowrap;
  background-color: #fff;
  bottom: 0px;
}

.commit-cart-quanxuan {
  margin-left: 20rpx;
