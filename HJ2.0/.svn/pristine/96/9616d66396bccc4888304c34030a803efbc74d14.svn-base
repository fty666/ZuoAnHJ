.header {
    width: 100%;
    background-color: #000;
}

.shehzhi {
    float: right;
}

.xiaoxi {
    float: right;
    margin-right: 20rpx;
    margin-left: 40rpx;
}

.profile {
    width: 100%;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    margin-left: 20rpx;
}

.profile .image {
    border-radius: 50%;
    z-index: 9999;
}

.nickname {
    margin-left: 20rpx;
    font-size: 30rpx;
    color: #fff;
}

.header .ad {
    width: 100%;
    height: 120rpx;
    margin-top: 20rpx;
    text-align: center;
}

.header .ad image {
    width: 655rpx;
    height: 120rpx;
}

.myorder-font {
    display: flex;
    justify-content: space-between;
    padding: 33rpx 20rpx;
    box-sizing: border-box;
}

.myorder-font-1 {
    font-size: 30rpx;
    color: #333;
    line-height: 30rpx;
}

.myorder-font-2 {
    font-size: 25rpx;
    color: #999;
    line-height: 30rpx;
    margin-left: 50rpx;
}

.line {
    margin: 20rpx 0 0 20rpx;
    background-color: #ccc;
}

.four-wait {
    display: flex;
    justify-content: space-around;
}

.four-wait-each {
    position: relative;
    text-align: center;
    width: 178rpx;
    height: 100rpx;
    margin-top: 30rpx;
}

/* .four-wait-each image{
 
} */

.four-wait-each-font {
    font-size: 24rpx;
    color: #333;
}

.three-caozuo {
    display: flex;
    justify-content: space-around;
    padding-bottom: 50rpx;
    box-sizing: border-box;
}

.three-caozuo-each {
    margin-top: 30rpx;
    text-align: center;
}

.three-caozuo .three-caozuo-each image {
    margin-top: 12rpx;
}

.bottom {
    width: 100%;
    margin-bottom: 10rpx;
    background-color: #eee;
}

.bottom-ad {
    text-align: center;
}

.bottom-ad image {
    margin-top: 20rpx;
}

.membership-privilege {
    margin-top: 20rpx;
    text-align: center;
}

.membership-privilege-font {
    font-size: 30rpx;
    margin-bottom: 20rpx;
}

.membership-privilege-scroll {
    position: relative;
    width: 160rpx;
    height: 200rpx;
    display: inline-block;
    margin-right: 10rpx;
}

.membership-privilege-scroll image {
    width: 160rpx;
    height: 200rpx;
    z-index: 1;
}

.membership-privilege-scroll view {
    position: absolute;
    top: 160rpx;
    left: 0rpx;
    width: 100%;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    font-size: 24rpx;
    color: #fff;
    background-color: #666;
    opacity: 0.5;
    z-index: 9999;
}
