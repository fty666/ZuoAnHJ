<view>
    <view class="header" style="background-image:url('https://zuoanzac.oss-cn-beijing.aliyuncs.com/userBj.png');">
        <navigator url="../users/users/users">
            <view class="profile" style="height:{{px2rpxHeight*240}}px;">
                <view>
                    <block wx:if="{{userInfo.photo.length<23}}">
                        <image src="{{serverUrl+userInfo.photo}}" class='image' style="width: {{px2rpxWidth*163}}px;height: {{px2rpxWidth*163}}px;"></image>
                    </block>
                    <block wx:else>
                        <image src="{{userInfo.photo}}" class='image' style="width: {{px2rpxWidth*163}}px;height: {{px2rpxWidth*163}}px;"></image>
                    </block>
                </view>

                <!-- <view class="nickname">还没有设置昵称</view> -->
                <view class="nickname">{{userInfo.nickName}}</view>
            </view>
        </navigator>
        <!-- <view class="ad">
            <image src="../../images/nav_right_adv.png"></image>
        </view> -->
    </view>
    <view class="myorder">
        <view class="myorder-font" style="width: 100%;height: {{px2rpxHeight*75}}px;">
            <view class="myorder-font-1">我的订单</view>
            <navigator url="/pages/myorder/myorder?dealerCode={{userInfo.dealerCode}}">
                <view class="myorder-font-2">查看全部订单 〉</view>
            </navigator>
        </view>
        <view class="line" style="width: 93%;height:{{px2rpxHeight*2}}px;"></view>
        <view class="four-wait">
            <view class="four-wait-each" style="width:{{px2rpxWidth*178}}px;height:{{px2rpxHeight*100}}px;">
                <navigator url="/pages/myorder/myorder?status={{pending_payment}}">
                    <view>
                        <image src="https://zuoanzac.oss-cn-beijing.aliyuncs.com/daifukuan.png" style="width:{{px2rpxWidth*50}}px;height:{{px2rpxWidth*50}}px;"></image>
                    </view>
                    <view class="four-wait-each-font">待付款</view>
                </navigator>
            </view>

            <view class="four-wait-each" style="width:{{px2rpxWidth*178}}px;height:{{px2rpxWidth*100}}px;">
                <navigator url="/pages/myorder/myorder?status={{to_be_shipped}}">
                    <view>
                        <image src="https://zuoanzac.oss-cn-beijing.aliyuncs.com/daifahuo.png" style="width:{{px2rpxWidth*50}}px;height:{{px2rpxHeight*50}}px;"></image>
                    </view>
                    <view class="four-wait-each-font">待发货</view>
                </navigator>
            </view>

            <view class="four-wait-each" style="width:{{px2rpxWidth*178}}px;height:{{px2rpxWidth*100}}px;">
                <navigator url="/pages/myorder/myorder?status={{to_be_received}}">
                    <view>
                        <image src="https://zuoanzac.oss-cn-beijing.aliyuncs.com/daishouhuo.png" style="width:{{px2rpxWidth*50}}px;height:{{px2rpxHeight*50}}px;"></image>
                    </view>
                    <view class="four-wait-each-font">待收货</view>
                </navigator>
            </view>
            <view class="four-wait-each" style="width:{{px2rpxWidth*178}}px;height:{{px2rpxWidth*100}}px;">
                <navigator url="/pages/myorder/myorder?status={{to_be_evaluated}}">
                    <view>
                        <image src="https://zuoanzac.oss-cn-beijing.aliyuncs.com/daipingjia.png" style="width:{{px2rpxWidth*50}}px;height:{{px2rpxHeight*50}}px;"></image>
                    </view>
                    <view class="four-wait-each-font">待评价</view>
                </navigator>
            </view>


        </view>
    </view>
    <view class="three-caozuo">

        <view class="three-caozuo-each" style="width: {{px2rpxWidth*236}}px;height: {{px2rpxHeight*130}}px;" catchtap='guanli'>
            <view>
                <image src="https://zuoanzac.oss-cn-beijing.aliyuncs.com/Add.png" style="width: {{px2rpxHeight*56}}px;height:{{px2rpxHeight*56}}px;"></image>
            </view>
            <view class="four-wait-each-font">收货地址</view>
        </view>

        <view class="three-caozuo-each" style="width: {{px2rpxWidth*236}}px;height: {{px2rpxHeight*130}}px;" catchtap='collects'>
            <view>
                <image src="https://zuoanzac.oss-cn-beijing.aliyuncs.com/collection.png" style="width: {{px2rpxHeight*56}}px;height:{{px2rpxHeight*56}}px;"></image>
            </view>
            <view class="four-wait-each-font">收藏</view>
        </view>

        <view class="three-caozuo-each" bindtap='custom' style="width: {{px2rpxWidth*236}}px;height: {{px2rpxHeight*130}}px;">
            <view>
                <image src="https://zuoanzac.oss-cn-beijing.aliyuncs.com/kefu.png" style="width: {{px2rpxHeight*56}}px;height:{{px2rpxHeight*56}}px;"></image>
            </view>
            <view class="four-wait-each-font">客服</view>
        </view>

    </view>

    <view class="jianju" style="background-color:#eee;margin-top:20rpx;height:20rpx;"></view>
    <!--商品列表  -->
    <view>
        <view style='height:{{px2rpxHeight*100}}px;text-align:center;line-height:90rpx;font-size:30rpx;'>精品推荐</view>
        <view style='display:flex'>
            <block wx:for="{{cakes}}" wx:key="{{index}}">
                <navigator url="/pages/goods_detail/goods_detail?id={{item.id}}">
                    <view>
                        <image src='{{serverUrl+item.img}}' style='width:{{px2rpxWidth*350}}rpx;height:{{px2rpxWidth*500}}rpx;margin-left:5rpx;'></image>
                        <view style='color:white;background:black;opacity: 0.5;width:{{px2rpxWidth*360}}rpx;height:{{px2rpxWidth*100}}rpx;margin-left:10rpx;text-align:center;margin-top:-36rpx;'>￥{{item.price}}</view>
                    </view>
                </navigator>
            </block>
        </view>
    </view>

    <view class="bottom" style='margin-bottom:98rpx;'>
        <view class="bottom-ad" style='height:{{px2rpxHeight*450}}rpx;'>
            <image src="https://zuoanzac.oss-cn-beijing.aliyuncs.com/buy.png" style="width:{{px2rpxWidth*710}}px;height:{{px2rpxHeight*180}}px;margin-top:{{px2rpxHeight*17}}px;" catchtap='buy'></image>
        </view>
    </view>

    <import src="../../template/template.wxml" />
    <template is="tabBar" data="{{tabBar:bindData.tabBar}}" />
</view>