.swiper {
    text-align: center;
    width: 100%;
    height: 750rpx;
    /* border: 1px solid red;  */
}

.swiper swiper {
    width: 100%;
    height: 100%;
}

.swiper image {
    width: 100%;
    height: 750rpx;
    /* border: 1px solid red; */
}

.goods-name {
    /* padding: 20rpx; */
    margin-top: 20rpx;
    font-size: 30rpx;
    color: #333;
    margin-left: 30rpx;
}

.caiNmae{
  font-size: 30rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.price-cart {
    display: flex;
    justify-content: flex-start;
    /* padding-left: 20rpx;  *//* margin-bottom: 20rpx; */
    font-size: 40rpx;
    color: #ec2525;
   
}

.member-price {
    display: flex;
    justify-content: center;
    align-items: center;
    background: #d83030;
    color: white;
    font-size: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid red;
}

.costprice {
    display: flex;
    justify-content: center;
    align-items: center;
    background: white;
    color: #ddd;
    font-size: 35rpx;
    text-decoration: line-through;
    border: 1rpx solid red;
}

.member-price2 {
    font-size: 26rpx;
    color: #b49e7d;
    padding-top: 10rpx;
    margin-left: 30rpx;
}

.stock {
    display: flex;
    justify-content: flex-start;
    padding-left: 20rpx;
    font-size: 28rpx;
    color: #bbb;
}

.stock .selling {
    margin-left: 65rpx;
}

.number {

    /* padding:20rpx;  */
    /* box-sizing: border-box; */
    font-size: 40rpx;
    line-height: 50rpx;
    color: #777;
    /* border: 1px solid red;  */
}

.jian, .num, .jia {

    border: 2rpx solid #ccc;
    text-align: center;
    line-height: 50rpx;
    font-size: 50rpx;
    float: left;
}

.jian {
    margin-left: 30rpx;
    border-radius: 5rpx 0 0 5rpx;
}

.jia {
    border-radius: 0 5rpx 5rpx 0;
}

.num {
    z-index: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 80rpx;
    border-left: 0;
    border-right: 0;
    font-size: 30rpx;
}

.address {
    /* padding-top: 20rpx; */
    font-size: 30rpx;
    color: #666;
    display: flex;
    /* border: 1px solid red;   */
}

.fonts {
    /* margin-left: -280rpx; */
    /* color:red;  */
    padding: 0 20rpx;
    box-sizing: border-box;
    line-height: 80rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.imgs {
    padding-top: 27rpx;
}

.address view {
    float: right;
    color: #aaa;
}

.msg {
    margin-left: 110rpx;
    font-size: 24rpx;
    color: #999;
    margin-bottom: 25rpx;
    /* border: 1px solid red;   */
}

.comment {
    display: flex;
    justify-content: space-between;
    padding: 30rpx 20rpx;
    box-sizing: border-box;
    font-size: 30rpx;
    color: #666;
}

.comment-img {
    display: flex;
    justify-content: flex-start;
    margin-right: 10rpx;
    margin-bottom: 30rpx;
    padding: 0 10rpx;
    flex-wrap: wrap;
}

/* .comment-img image {

} */

.bottom-btn {
    position: fixed;
    top: 91.7%;
    left: 0;
    display: flex;
    flex-direction: row;
    background-color: #fff;
}

.custom-service {
    width: 185rpx;
    height: 100rpx;
    padding-top: 10rpx;
    text-align: center;
    /* line-height: 100rpx; */
    font-size: 25rpx;
    color: #666;
    box-sizing: border-box;
    /* border: 1px solid red; */
}

.carts {
    padding-top: 10rpx;
    text-align: center;
    /* line-height: 100rpx; */
    font-size: 25rpx;
    color: #666;
    box-sizing: border-box;
    margin-left: -22rpx;
    /* border: 1px solid red; */
}

.custom-service image {
    width: 45rpx;
    height: 45rpx;
}

.ziti {
    line-height: 80rpx;
    font-size: 35rpx;
    color: #fff;
    background-color: #ffb97a;
}

.immediate-sale {
    line-height: 80rpx;
    font-size: 35rpx;
    color: #fff;
    background-color: #ce000d;
}

.guess {
    margin: 0 0 20rpx 20rpx;
    font-size: 30rpx;
    color: #666;
}

.youlike {
    margin-left: 20rpx;
}

.youlike image {
    width: 217rpx;
    height: 220rpx;
}

.youlike view {
    width: 200rpx;
    font-size: 26rpx;
    color: #999;
}

.youlike-each {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    float: left;
    width: 230rpx;
