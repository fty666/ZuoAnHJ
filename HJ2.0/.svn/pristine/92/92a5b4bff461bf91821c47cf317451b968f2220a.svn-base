<view>

  <!--距离顶部  -->
  <view class="padd" style='height:{{px2rpxHeight*20}}px;'></view>
  <block wx:if="{{sell==''}}">
    <view class='none'>您还没有自提过的商品</view>
  </block>
  <block wx:else>
    <!--循环出  -->
    <block wx:for="{{sell}}" wx:key="{{item.id}}">
      <view>
        <view style='height:{{px2rpxHeight*220}}px;'>
          <!--图片  -->
          <view class='picture'>
            <image src="{{serverUrl+item.goodsImg}}" class='picture' style='width:{{px2rpxHeight*160}}px;height:{{px2rpxHeight*160}}px;'></image>
          </view>

          <!--内容  -->
          <view calss="content" style='height:{{px2rpxHeight*200}}px;'>
            <view class="name"> {{item.goodsName}}*规格{{item.specification}}</view>
            <view class="price" style='height:{{px2rpxHeight*60}}px;'> ¥{{item.price}}</view>
            <!-- <view class="text">
                        <view class='info'>销售中:{{item.dealer.num}}</view>
                        <view class='info'>已售:{{item.saleNum.num}}</view>
                        <view class='info'>库存:{{item.dealer.possession}}</view>
                    </view> -->
          </view>

          <!--订单号  时间  -->
          <view style='display:flex;margin-top:60rpx;'>
            <view class='font'>订单号：{{item.uuid}}</view>
            <view class='font2'>{{item.createTime}}</view>
          </view>

          <!--下划线  -->
          <view class="xia"></view>
        </view>
      </view>
    </block>
  </block>
</view>