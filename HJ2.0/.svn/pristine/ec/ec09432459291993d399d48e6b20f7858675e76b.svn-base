<view>
  <!--距离顶部  -->
  <view class="padd" style='height:{{px2rpxHeight*20}}px;'></view>
  <!--内容1  -->
  <view class="content" style='height:{{px2rpxHeight*180}}px;'>
    <view class="zhanhu">账户余额</view>
    <view class="date">
      <view bindtap='back' class='an'>
        <image src='../../../images/left.png' class='left' style='width:{{px2rpxHeight*20}}px;height: {{px2rpxHeight*20}}px;'></image>
      </view>
      <view>
        <text decode='{{true}}' space='{{true}}'>&nbsp;&nbsp;&nbsp;{{Ytime}}年{{Mtime}}月&nbsp;&nbsp;&nbsp;</text>
      </view>
      <view bindtap='go' class='an'>
        <image src='../../../images/right.png' style='width:{{px2rpxHeight*20}}px;height: {{px2rpxHeight*20}}px;' class='left'></image>
      </view>
    </view>

    <view style="clear:both"></view>

    <!--金额  -->
    <view>
      <view class="jine">￥{{sumprices}} </view>
      <view class="ti" catchtap='cash' style="width:{{px2rpxWidth*162}}px;height:{{px2rpxHeight*60}}px;">提现 </view>
      <view class="gou">
        <navigator url='/pages/trader/index/index'> 继续购买</navigator>
      </view>
    </view>
  </view>

  <!--内容 2  -->
  <view class="padd"></view>
  <view class="content">
    <view style='display:flex;'>
      <view class='yprice'>￥ {{yprice}}</view>
      <view class='sprice'>￥ {{sumprices}}</view>
    </view>
    <view>
      <text decode='{{true}}' space='{{true}}' style='font-size:30rpx;'>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;本月成交额&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;总成交额&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</text>
    </view>
  </view>

  <view class="padd"></view>

  <!--内容3  -->
  <view class="zongti">
    <!--订单明细  -->
    <view style="height:{{px2rpxHeight*80}}px;">
      <view style='display:flex;justify-content:flex-start;'>
        <image src="../../../images/yeji.png" style='width:{{px2rpxHeight*40}}px;height:{{px2rpxHeight*36}}px;margin-left:30rpx;margin-top:20rpx;'></image>
        <text decode='{{true}}' space="{{true}}" style="font-size:30rpx;margin-top:20rpx;">&nbsp;&nbsp;&nbsp;&nbsp;订单明细</text>
        <!-- <image src="../../../images/float.png"  style='width:28rpx;height:28rpx;margin-left:430rpx;margin-top:20rpx;'></image> -->
      </view>
    </view>

    <!--具体记录  -->
    <view class="zongti2" style='height:{{px2rpxHeight*320}}px;' wx:for="{{order}}" wx:key="{{item.id}}">
      <!--下划线  -->
      <view class="xia"></view>
      <view class="picture">
        <image src="{{serverUrl}}{{item.img}}" style='width:{{px2rpxHeight*150}}px;height:{{px2rpxHeight*150}}px;'></image>
      </view>
      <view class='name'>{{item.goodsName}}{{item.specification}}</view>
      <view style='clear:both'></view>
      <!--金额  -->
      <view class='order'>
        <text decode='{{true}}' space='{{true}}' class="text">&nbsp;&nbsp;&nbsp;总金额&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;成交预估收入&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;提成</text>
      </view>

      <view style='display:flex;'>
        <view class='pay'>{{item.soldPrice}}</view>
        <view class='pay1'>{{item.soldPrice}}</view>
        <view class='pay2'>{{item.gain}}</view>
      </view>

      <!--时间  -->
      <view class='jsdate'>
        <text decode='{{true}}' space='{{true}}'>{{times}}&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;结算</text>
      </view>
    </view>
  </view>
</view>