page{
    background-color: #f0f0f0;
}
.nav{
    display: flex;
    justify-content: space-around;
     padding:0 20rpx; 
     box-sizing: border-box; 
    background-color: #fff;
}
.nav-each{
    display: flex;
    justify-content: center;
    align-items: center;
    /* width: 178rpx;
    height: 88rpx; */
}
.nav-each text{
    margin-right: 10rpx;
}

.goods-list-all{
    float: left;
    margin:20rpx 0 0 20rpx; 
}
.goods-list-each{
    padding: 20rpx;
    box-sizing: border-box;
    background-color: #fff;
}
.goods-name{
    margin-top:10rpx; 
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 26rpx;
    color: #333;
}
.goods-price{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 20rpx;
    font-size: 30rpx;
    color: #ec2525;
}
