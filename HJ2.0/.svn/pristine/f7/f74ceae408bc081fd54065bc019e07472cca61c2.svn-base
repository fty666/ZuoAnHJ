<view style="margin-top:20rpx;">
  <!-- <block wx:for="{{commit_order}}">  -->
  <view class="consignee" style='height:{{px2rpxHeight*170}}px;'>
    <view class="consignee-font1">收货人： {{address.receiveName}}
      <text style='margin-left:80rpx;'>{{address.phone}}</text> </view>
    <view class="consignee-font2">{{address.addrDetail}}</view>
    <view bindtap='selectAddress' class="consignee-img">
      <image src="../../images/fanhui.png" style='width:{{px2rpxHeight*13}}px;height:{{px2rpxHeight*24}}px;' />
    </view>
  </view>
  <view style="margin-top:20rpx;">
    <view sytle="width:100%;background-color:#fff;">
      <block wx:for="{{order.goodsInfo}}" wx:for-item="vv" wx:key="{{vv.gid}}">
        <view class="myorder_each">
          <view class="goods-info" style='width:100%;height:{{px2rpxHeight*160}}px;'>
            <view class="goods-img">
              <image src="{{serverUrl + vv.img}}" style='width:{{px2rpxHeight*120}}px;height:{{px2rpxHeight*120}}px;'></image>
            </view>
            <view class="goods-name" style='width:{{px2rpxHeight*434}}px;height:{{px2rpxHeight*100}}px;'>
              <view>
                {{vv.name}}
              </view>
            </view>
            <view class="goods-price" style='width:{{100*px2rpxHeight}}px;height:{{px2rpxHeight*150}}px;'>
              <view class="buy-price">¥{{vv.price}}</view>
              <view class="huaxian-price"></view>
              <view class="buy-number">x{{goods_num}}</view>
            </view>
          </view>
        </view>
      </block>
    </view>
  </view>
  <!-- <view class="invoice-information">
            发票信息 
            <input bindblur='receiptInfo' type='text' placeholder="不需要发票" value=''></input>
        </view> -->
  <view class="remarks" style='width:{{px2rpxHeight*740}}px;height:{{px2rpxHeight*160}}px;'>
    <textarea bindblur='msg' style='width:{{px2rpxHeight*700}}px;height:{{px2rpxHeight*160}}px;' placeholder="备注" auto-focus="true" show-confirm-bar="false"></textarea>
  </view>
  <view class="goods-total">
    <view class="goods-total-font1">商品总价</view>
    <view class="goods-total-font2">¥{{order.soldPrice}}</view>
  </view>
  <!-- <view class="goods-total" style='margin-top:0;'>  -->
  <!-- <view class="goods-total-font1">运费</view> -->
  <!-- <view class="goods-total-font2">¥{{commit_order.transportation_expenses}}</view> -->
  <!-- </view> -->
  <view class="qurenziti" style='width: 100%;height:{{px2rpxHeight*98}}px;'>
    <view class="qurenziti-font" style='width:{{px2rpxHeight*450}}px;height:{{px2rpxHeight*98}}px;'>总计
      <text> ¥{{order.soldPrice}}</text>
    </view>
    <view bindtap='commitOrder' class="qurenziti-btn" style='width:{{px2rpxHeight*300}}px;height:{{px2rpxHeight*98}}px;'>确认自提</view>
  </view>
  <!-- </block>  -->
</view>